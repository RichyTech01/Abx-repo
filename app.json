{
  "expo": {
    "name": "abx-mobile",
    "slug": "abx-mobile-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/Images/MainLogo.png",
    "scheme": "abxmobileapplication",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0C513F"
    },
    "newArchEnabled": true,
    "extra": {
      "GET_ADDRESS_API_KEY": "LmRcb8Qk50mEojODY5sv3w46906",

      "router": {},
      "eas": {
        "projectId": "8e52f21a-83fe-434f-ab77-05345693e72a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show nearby stores.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.henry23.abxmobileapp"
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/MainLogo.png",
        "backgroundColor": "#0C513F"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.henry23.abxmobileapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/Images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#0C513F"
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them in chat."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
